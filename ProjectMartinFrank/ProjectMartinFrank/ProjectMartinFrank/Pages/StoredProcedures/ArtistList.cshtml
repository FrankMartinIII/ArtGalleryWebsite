@page
@model ProjectMartinFrank.Pages.StoredProcedures.ArtistListModel
@{
}

<form class="form-horizontal" method="post" id="moi">
    <div class="form-group">
        <label for="lname" class="col-sm-2 control-label">Last Name:</label>
        <div class="col-sm-10">
            <input type="text" id="lname" class="form-control" value="" name="lname">
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-primary">Search</button>
        </div>
    </div>
</form>


<table id="galTable" class="artTable table">
    <thread>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.spArtistLists[0].artistFName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.spArtistLists[0].artistLName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.spArtistLists[0].artistDOB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.spArtistLists[0].artistEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.spArtistLists[0].AmountArtUploaded)
            </th>
        </tr>
    </thread>
    <tbody>
        @foreach (var item in Model.spArtistLists)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.artistFName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.artistLName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.artistDOB)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.artistEmail)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AmountArtUploaded)
                </td>
            </tr>
        }
    </tbody>
</table>